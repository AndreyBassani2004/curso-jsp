
//Criacao da tabela

CREATE TABLE public."Model_Login"
(
  login character varying(200),
  senha character varying(200),
  CONSTRAINT login_unique UNIQUE (login)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE public."Model_Login"
  OWNER TO postgres;
  
  
